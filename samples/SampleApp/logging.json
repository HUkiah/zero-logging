{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": "true"
    },
    "File": {
      // 按分钟滚动写入
      "RollingInterval": "Minute"
    },
    "Elasticsearch": {
      "AutoRegisterTemplate": true
    }
  }
}